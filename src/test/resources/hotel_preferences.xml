<?xml version="1.0" encoding="UTF-8" ?>
<PREFERENCE-SPECIFICATION>

  <!-- Variables -->
  <PREFERENCE-VARIABLE>
    <VARIABLE-NAME>W</VARIABLE-NAME>
    <DOMAIN-VALUE>Wy</DOMAIN-VALUE>
    <DOMAIN-VALUE>Wn</DOMAIN-VALUE>
  </PREFERENCE-VARIABLE>
  <PREFERENCE-VARIABLE>
    <VARIABLE-NAME>R</VARIABLE-NAME>
    <DOMAIN-VALUE>Rl</DOMAIN-VALUE>
    <DOMAIN-VALUE>Rm</DOMAIN-VALUE>
    <DOMAIN-VALUE>Rs</DOMAIN-VALUE>
  </PREFERENCE-VARIABLE>
  <PREFERENCE-VARIABLE>
    <VARIABLE-NAME>B</VARIABLE-NAME>
    <DOMAIN-VALUE>Bo</DOMAIN-VALUE>
    <DOMAIN-VALUE>Bn</DOMAIN-VALUE>
  </PREFERENCE-VARIABLE>
  <PREFERENCE-VARIABLE>
    <VARIABLE-NAME>C</VARIABLE-NAME>
    <DOMAIN-VALUE>Cy</DOMAIN-VALUE>
    <DOMAIN-VALUE>Cn</DOMAIN-VALUE>
  </PREFERENCE-VARIABLE>
  <PREFERENCE-VARIABLE>
    <VARIABLE-NAME>P</VARIABLE-NAME>
    <DOMAIN-VALUE>Pl</DOMAIN-VALUE>
    <DOMAIN-VALUE>Ps</DOMAIN-VALUE>
  </PREFERENCE-VARIABLE>

  <!-- Preferences -->
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefW</STATEMENT-ID>
    <PREFERENCE-VARIABLE>W</PREFERENCE-VARIABLE>
    <PREFERENCE>Wy:Wn</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefR</STATEMENT-ID>
    <PREFERENCE-VARIABLE>R</PREFERENCE-VARIABLE>
    <PREFERENCE>Rl:Rm</PREFERENCE>
    <PREFERENCE>Rm:Rs</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefB1</STATEMENT-ID>
    <PREFERENCE-VARIABLE>B</PREFERENCE-VARIABLE>
    <CONDITION>R=Rs</CONDITION>
    <PREFERENCE>Bo:Bn</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefB2</STATEMENT-ID>
    <PREFERENCE-VARIABLE>B</PREFERENCE-VARIABLE>
    <CONDITION>R=Rm</CONDITION>
    <PREFERENCE>Bn:Bo</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefB3</STATEMENT-ID>
    <PREFERENCE-VARIABLE>B</PREFERENCE-VARIABLE>
    <CONDITION>R=Rl</CONDITION>
    <PREFERENCE>Bn:Bo</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefC</STATEMENT-ID>
    <PREFERENCE-VARIABLE>C</PREFERENCE-VARIABLE>
    <PREFERENCE>Cy:Cn</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefP1</STATEMENT-ID>
    <PREFERENCE-VARIABLE>P</PREFERENCE-VARIABLE>
    <CONDITION>C=Cy</CONDITION>
    <CONDITION>B=Bn</CONDITION>
    <PREFERENCE>Pl:Ps</PREFERENCE>
  </PREFERENCE-STATEMENT>
  <PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefP2</STATEMENT-ID>
    <PREFERENCE-VARIABLE>P</PREFERENCE-VARIABLE>
    <CONDITION>C=Cn</CONDITION>
    <PREFERENCE>Ps:Pl</PREFERENCE>
  </PREFERENCE-STATEMENT><PREFERENCE-STATEMENT>
    <STATEMENT-ID>prefP3</STATEMENT-ID>
    <PREFERENCE-VARIABLE>P</PREFERENCE-VARIABLE>
    <CONDITION>B=Bo</CONDITION>
    <PREFERENCE>Ps:Pl</PREFERENCE>
  </PREFERENCE-STATEMENT>

</PREFERENCE-SPECIFICATION>
